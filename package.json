  "license": "MIT",
  "scripts": {
    "client:start": "react-scripts start",
    "server:start": "ts-node --project src/server/tsconfig.json src/server/main.ts",
    "server:start:dev": "ts-node-dev --project src/server/tsconfig.json src/server/main.ts",
    "test:types": "tsc",
    "test:lint": "eslint src/ --ext .jsx,.js,.ts,.tsx --color",
    "test:prettier": "prettier --check \"./{src,types,scripts}/**/*.{js,jsx,json,ts,tsx,scss}\"",
    "fix:lint": "eslint src/ --ext .jsx,.js,.ts,.tsx --color --fix",
    "fix:prettier": "prettier --write \"./{src,types,scripts}/**/*.{js,jsx,json,ts,tsx,scss}\""
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "debug": "^4.3.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "js-base64": "^3.6.0",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/js-base64": "^3.0.0",
    "@types/node": "^14.14.22",
    "@types/ramda": "^0.27.36",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7"
  }
}
